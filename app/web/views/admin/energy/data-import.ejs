<div class="row mb-3">
    <div class="col-md-6">
        <h3>Data Import</h3>
    </div>
</div>
<div class="row">
    <div class="col-md-auto">
        <table class="table table-bordered">
            <tr>
                <th>Provider</th>
                <th>Setup</th>
            </tr>
            <% for (let api of apis) { %>
                <tr>
                    <td><%= api.name %></td>
                    <td>
                        <ul>
                            <% for (let cred of api.credentials) { %>
                                <li><%= cred.key %>: <%= cred.value %></li>
                            <% } %>
                        </ul>
                    </td>
                </tr>
            <% } %>
        </table>
    </div>
</div>
